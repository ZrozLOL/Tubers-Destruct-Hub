	local isSpawning = false -- –ó–º—ñ–Ω–Ω–∞ –¥–ª—è –∫–æ–Ω—Ç—Ä–æ–ª—é —Å–ø–∞–≤–Ω—ñ–Ω–≥—É
	local spawnConnection -- –ó–º—ñ–Ω–Ω–∞ –¥–ª—è –∑–±–µ—Ä–µ–∂–µ–Ω–Ω—è –ø—ñ–¥–∫–ª—é—á–µ–Ω–Ω—è –¥–æ —Å–ø–∞–≤–Ω—ñ–Ω–≥—É

	-- –§—É–Ω–∫—Ü—ñ—è –¥–ª—è —Å—Ç–≤–æ—Ä–µ–Ω–Ω—è —á–∞—Å—Ç–∏–Ω–∏ (Part)
	local function createPart()
		local character = player.Character or player.CharacterAdded:Wait() -- –û—Ç—Ä–∏–º—É—î–º–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞ –≥—Ä–∞–≤—Ü—è
		local humanoidRootPart = character:WaitForChild("HumanoidRootPart") -- –í–∏–∫–æ—Ä–∏—Å—Ç–æ–≤—É—î–º–æ HumanoidRootPart —è–∫ —Ç–æ—á–∫—É —Å–ø–∞–≤–Ω—ñ–Ω–≥—É

		local part = Instance.new("Part") -- –°—Ç–≤–æ—Ä—é—î–º–æ –Ω–æ–≤–∏–π Part
		part.Size = Vector3.new(8.811, 4.305, 5.307) -- –í—Å—Ç–∞–Ω–æ–≤–ª—é—î–º–æ —Ä–æ–∑–º—ñ—Ä–∏
		part.Position = humanoidRootPart.Position + Vector3.new(0, 5, 0) -- –í—Å—Ç–∞–Ω–æ–≤–ª—é—î–º–æ –ø–æ–∑–∏—Ü—ñ—é –Ω–∞–¥ –≥—Ä–∞–≤—Ü–µ–º
		part.Anchored = false -- –ó–∞–∫—Ä—ñ–ø–ª—é—î–º–æ Part, —â–æ–± –≤—ñ–Ω –Ω–µ –ø–∞–¥–∞–≤
		part.Parent = workspace -- –î–æ–¥–∞—î–º–æ Part –≤ —Ä–æ–±–æ—á–∏–π –ø—Ä–æ—Å—Ç—ñ—Ä
	end

	-- –§—É–Ω–∫—Ü—ñ—è –¥–ª—è –∑–∞–ø—É—Å–∫—É –∞–±–æ –∑—É–ø–∏–Ω–∫–∏ —Å–ø–∞–≤–Ω—ñ–Ω–≥—É
	local function toggleSpawning()
		if isSpawning then
			trojanon.Text = "Turn On"
			-- –Ø–∫—â–æ –≤–∂–µ —Å–ø–∞–≤–Ω–∏—Ç—å—Å—è, –∑—É–ø–∏–Ω—è—î–º–æ —Å–ø–∞–≤–Ω—ñ–Ω–≥
			isSpawning = false
			if spawnConnection then
				spawnConnection:Disconnect() -- –í—ñ–¥–∫–ª—é—á–∞—î–º–æ –ø–æ–ø–µ—Ä–µ–¥–Ω—î –ø—ñ–¥–∫–ª—é—á–µ–Ω–Ω—è
			end
		else
			-- –Ø–∫—â–æ –Ω–µ —Å–ø–∞–≤–Ω–∏—Ç—å—Å—è, –∑–∞–ø—É—Å–∫–∞—î–º–æ —Å–ø–∞–≤–Ω—ñ–Ω–≥
			isSpawning = true
			trojanon.Text = "Turn Off"
			spawnConnection = game:GetService("RunService").Heartbeat:Connect(function()
				createPart() -- –í–∏–∫–ª–∏–∫–∞—î–º–æ —Ñ—É–Ω–∫—Ü—ñ—é —Å—Ç–≤–æ—Ä–µ–Ω–Ω—è —á–∞—Å—Ç–∏–Ω–∏
				wait(0.1) -- –ó–∞—Ç—Ä–∏–º–∫–∞ –≤ 1 —Å–µ–∫—É–Ω–¥—É –º—ñ–∂ —Å–ø–∞–≤–Ω–∞–º–∏
			end)
		end
	end

	-- –ü—Ä–∏–≤'—è–∑—É—î–º–æ —Ñ—É–Ω–∫—Ü—ñ—é –¥–æ –Ω–∞—Ç–∏—Å–∫–∞–Ω–Ω—è –∫–Ω–æ–ø–∫–∏
	button.MouseButton1Click:Connect(toggleSpawning)

	-- –ü—Ä–∏–ø—É—Å—Ç–∏–º–æ, —â–æ —É –≤–∞—Å —î –∫–Ω–æ–ø–∫–∞ –≤ GUI –ø—ñ–¥ –Ω–∞–∑–≤–æ—é "XRayButton"


	-- –û—Ç—Ä–∏–º—É—î–º–æ –ø–æ—Å–∏–ª–∞–Ω–Ω—è –Ω–∞ –Ω–µ–æ–±—Ö—ñ–¥–Ω—ñ –æ–±'—î–∫—Ç–∏
	-- –û—Ç—Ä–∏–º—É—î–º–æ –ø–æ—Å–∏–ª–∞–Ω–Ω—è –Ω–∞ –Ω–µ–æ–±—Ö—ñ–¥–Ω—ñ –æ–±'—î–∫—Ç–∏
	local textBox = EnterNameBox
	local button = Ok
	local players = game:GetService("Players")
	local localPlayer = players.LocalPlayer

	-- –§—É–Ω–∫—Ü—ñ—è –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü—ñ—ó –¥–æ –≥—Ä–∞–≤—Ü—è
	local function teleportToPlayer(partialName)
		local targetPlayer = nil

		-- –ü–µ—Ä–µ–±–∏—Ä–∞—î–º–æ –≤—Å—ñ—Ö –≥—Ä–∞–≤—Ü—ñ–≤ —ñ —à—É–∫–∞—î–º–æ —Ç–æ–≥–æ, —á–∏—î —ñ–º'—è –∞–±–æ –≤—ñ–¥–æ–±—Ä–∞–∂—É–≤–∞–Ω–∏–π –Ω—ñ–∫ –ø–æ—á–∏–Ω–∞—î—Ç—å—Å—è –∑ –≤–≤–µ–¥–µ–Ω–æ–≥–æ —Ç–µ–∫—Å—Ç—É
		for _, player in ipairs(players:GetPlayers()) do
			if player.Name:sub(1, #partialName):lower() == partialName:lower() or
				player.DisplayName:sub(1, #partialName):lower() == partialName:lower() then
				targetPlayer = player
				break
			end
		end

		if targetPlayer then
			local character = targetPlayer.Character
			if character and character:FindFirstChild("HumanoidRootPart") then
				localPlayer.Character:SetPrimaryPartCFrame(character.HumanoidRootPart.CFrame)
				ErrorFound.Text = "Successfully!"
				wait(2)
				ErrorFound.Text = ""
			else
				ErrorFound.Text = "Player Humanoid not found!"
				wait(2)
				ErrorFound.Text = ""
			end
		else
			ErrorFound.Text = "Player name or Player not found!"
			wait(2)
			ErrorFound.Text = ""
		end
	end

	-- –û–±—Ä–æ–±–∫–∞ –Ω–∞—Ç–∏—Å–∫–∞–Ω–Ω—è –Ω–∞ –∫–Ω–æ–ø–∫—É
	button.MouseButton1Click:Connect(function()
		local partialName = textBox.Text
		if partialName and partialName ~= "" then
			teleportToPlayer(partialName)
		else
			ErrorFound.Text = "Enter Name"
			wait(2)
			ErrorFound.Text = ""
		end
	end)

	local main = screenGui
	local Frame = frame


	local tis



	game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(char)
		wait(0.7)
		game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
		game.Players.LocalPlayer.Character.Animate.Disabled = false

	end)




	close.MouseButton1Click:Connect(function()
		close:Destroy()
		textLabel1.Text = "Bye!üëãüèΩ"
		textLabel1.TextColor3 = Color3.fromRGB(255, 255, 255)
		wait(3)
		screenGui:Destroy()
		wait(1)
		script.Enabled = false
	end)
	-- –î–æ–¥–∞—î–º–æ ScreenGui –≤ PlayerGui –≥—Ä–∞–≤—Ü—è
	screenGui.Parent = player:WaitForChild("PlayerGui")

	local player = game.Players.LocalPlayer
	local mouse = player:GetMouse()

	mouse.Button1Down:Connect(function()
		Holding.Value = true
	end)

	mouse.Button1Up:Connect(function()
		Holding.Value = false
	end)

	mouse.Move:Connect(function()
		if Holding.Value == true then
			frame.Position = UDim2.new(0,mouse.X,0,mouse.Y) -- Change "Frame" to your frame name
		end
	end)


	

end


-- –ü–µ—Ä–µ–∫–æ–Ω–∞—î–º–æ—Å—è, —â–æ –ø–µ—Ä—Å–æ–Ω–∞–∂ –≤–∂–µ —î
if player.Character then
	createScreenGuiForPlayer()
else
	-- –°—Ç–≤–æ—Ä—é—î–º–æ GUI, –∫–æ–ª–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂ –∑'—è–≤–∏—Ç—å—Å—è
	player.CharacterAdded:Connect(createScreenGuiForPlayer)
end
